params {
    ecr_registry = ''
    outdir = '/mnt/workflow/pubdir'
}

manifest {
    nextflowVersion = '!>=23.10'
}

conda {
    enabled = false
}

docker {
    enabled = true
    registry = params.ecr_registry //'<account-id>.dkr.ecr.<region>.amazonaws.com'
}

process {
withName: '.*' { conda = null }
withName: '(.+:)?SAMPLESHEET_CHECK' { container = 'biocontainers/python:3.8.3' }
withName: '(.+:)?CUSTOM_DUMPSOFTWAREVERSIONS' { container = 'biocontainers/multiqc:1.25.1--pyhdfd78af_0' }
withName: '(.+:)?FASTQC' { container = 'biocontainers/fastqc:0.12.1--hdfd78af_0' }
withName: '(.+:)?MULTIQC' { container = 'biocontainers/multiqc:1.25.1--pyhdfd78af_0' }
withName: '(.+:)?SEQTK' { container = 'staphb/seqtk:1.4' }
}
